{
  "manifest_version": 2,
  "name": "JSONView",
  "version": "2.0.0",
  "description": "View JSON documents in the browser.",
  "author": "Benjamin Hollis",
  "homepage_url": "https://jsonview.com/",
  "icons": {
    "32": "icon.png",
    "64": "icon64.png",
    "256": "icon256.png"
  },
  "background": {
    "scripts": ["main.js"]
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "applications": {
    "gecko": {
      "id": "jsonview@brh.numbera.com"
    }
  }
}
